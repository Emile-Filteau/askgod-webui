{
  "name": "askgod-webui",
  "version": "2.0.0",
  "description": "AskGod Web UI",
  "author": "kmarin@nsec.io",
  "private": false,
  "scripts": {
    "dev": "nuxt",
    "build": "npm run build:nsec-ctf",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "build:nsec-ctf": "DEPLOY_ENV=NSEC_CTF nuxt build",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "generate": "nuxt generate",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "serve:api": "json-server ./test/api/server.js --port 3004 --routes ./test/api/routes.json",
    "serve:ws": "node ./test/websocket-server-test.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/moment": "^1.6.1",
    "chart.js": "^2.9.3",
    "emoji-flags": "^1.3.0",
    "faker": "^4.1.0",
    "moment": "^2.25.3",
    "nuxt": "^2.12.2",
    "vue-chartjs": "^3.5.0",
    "vuetify": "^2.2.27",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^4.7.1",
    "jest": "^26.0.1",
    "jest-serializer-vue": "^2.0.2",
    "json-server": "^0.16.1",
    "push-dir": "^0.4.1",
    "vue-jest": "^3.0.5",
    "ws": "^6.2.1"
  }
}
