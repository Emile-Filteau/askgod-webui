{
  "name": "askgod-webui-2018",
  "version": "1.0.0",
  "description": "AskGod Web UI",
  "author": "kevenmarin@gmail.com",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "build:nsec-ctf": "DEPLOY_ENV=NSEC_CTF nuxt build",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "generate": "nuxt generate",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "NODE_ENV=test ava --serial --verbose",
    "test:ci": "NODE_ENV=test ava --serial --verbose --watch",
    "test:coverage": "nyc ava"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/moment": "^1.0.0",
    "ajv": "^6.4.0",
    "chart.js": "^2.7.2",
    "moment": "^2.22.1",
    "nuxt": "^2.12.2",
    "vue-chartjs": "^3.3.1",
    "vuetify": "^1.0.0",
    "vuex-persistedstate": "^2.5.2"
  },
  "devDependencies": {
    "@nuxtjs/proxy": "^1.3.3",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-imports": "^1.4.1",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.5.0",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ws": "^6.1.0"
  },
  "babel": {
    "presets": [
      "vue-app"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
